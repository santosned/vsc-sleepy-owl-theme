{
	"name": "theme-sleepy-owl",
	"displayName": "Sleepy Owl",
	"description": "A dark theme for vscode.",
	"version": "1.0.0",
	"publisher": "santosned",
	"scripts": {
		"build": "node ./scripts/build.js",
		"test": "node ./scripts/test.js",
		"package": "vsce package -o ./build/Release/sleepy-owl.vsix",
		"publish": "vsce publish"
	},
	"contributes": {
		"themes": [
			{
				"label": "Sleepy Owl",
				"uiTheme": "vs-dark",
				"path": "./themes/sleepy-owl-color-theme.json"
			}
		]
	},
	"maintainers": [
		"Eduardo M. Santos"
	],
	"contributors": [],
	"devDependencies": {
		"js-yaml": "^4.1.0",
		"vsce": "^2.7.0"
	},
	"engines": {
		"vscode": "^1.66.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/santosned/vsc-sleepy-owl-theme.git"
	},
	"bugs": {
		"url": "https://github.com/santosned/vsc-sleepy-owl-theme/issues"
	},
	"galleryBanner": {
		"theme": "dark"
	},
	"icon": "icon.png",
	"license": "MIT",
	"categories": [
		"Themes"
	],
	"keywords": [
		"color-theme",
		"dark",
		"owl",
		"sleepy",
		"theme"
	],
	"__metadata": {
		"publisherDisplayName": "Sleepy Owl Theme"
	}
}
