{
	"name": "theme-sleepy-owl",
	"displayName": "Sleepy Owl (Beta)",
	"description": "Collection of dark themes for coding overnight.",
	"version": "0.65.0",
	"publisher": "santosned",
	"scripts": {
		"build": "node ./scripts/build.js",
		"start": "node ./scripts/start.js",
		"test": "node ./scripts/test.js",
		"package": "node ./scripts/package.js && vsce package -o ./themes/release/sleepy-owl.vsix",
		"publish": "vsce publish"
	},
	"contributes": {
		"themes": [
			{
				"label": "Sleepy Owl - Cadet Default (Beta)",
				"uiTheme": "vs-dark",
				"path": "./themes/sleepy-owl-cadet-default-color-theme.json"
			},
			{
				"label": "Sleepy Owl - Mint (Beta)",
				"uiTheme": "vs-dark",
				"path": "./themes/sleepy-owl-mint-color-theme.json"
			},
			{
				"label": "Sleepy Owl - Wine (Beta)",
				"uiTheme": "vs-dark",
				"path": "./themes/sleepy-owl-wine-color-theme.json"
			},
			{
				"label": "Sleepy Owl - Onyx (Beta)",
				"uiTheme": "vs-dark",
				"path": "./themes/sleepy-owl-onyx-color-theme.json"
			}
		]
	},
	"maintainers": [
		"Eduardo M. Santos"
	],
	"contributors": [],
	"devDependencies": {
		"js-yaml": "^4.1.0",
		"vsce": "^2.7.0"
	},
	"engines": {
		"vscode": "^1.66.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/santosned/vsc-sleepy-owl-theme.git"
	},
	"bugs": {
		"url": "https://github.com/santosned/vsc-sleepy-owl-theme/issues"
	},
	"galleryBanner": {
		"theme": "dark"
	},
	"icon": "assets/icon.png",
	"license": "MIT",
	"categories": [
		"Themes"
	],
	"keywords": [
		"color-theme",
		"dark",
		"owl",
		"sleepy",
		"theme"
	],
	"__metadata": {
		"publisherDisplayName": "Sleepy Owl Theme"
	}
}
